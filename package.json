{
   "name": "dark-mode-tailwind",
   "version": "0.0.1",
   "description": "Package to implement dark mode to an existing tailwindCSS project",
   "keywords": [
      "tailwind",
      "tailwindcss",
      "dark tailwind",
      "dark tailwindcss",
      "dark mode tailwind",
      "dark mode tailwindcss",
      "darkmode tailwind",
      "darkmode tailwindcss",
      "dark-mode-tailwind",
      "dark-mode-tailwindcss"
   ],
   "author": "Lukasz Maraj",
   "license": "MIT",
   "private": false,
   "main": "dist/index.js",
   "module": "dist/index.mjs",
   "types": "dist/index.d.ts",
   "files": [
      "dist",
      "README.md",
      "LICENSE"
   ],
   "scripts": {
      "dev": "vitest",
      "test": "vitest run",
