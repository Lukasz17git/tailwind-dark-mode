   "bugs": {
      "url": "https://github.com/Lukasz17git/tailwind-dark-mode/issues"
   },
   "devDependencies": {
      "@changesets/cli": "^2.26.2",
      "csstype": "^3.1.2",
      "tailwindcss": "^3.3.3",
      "tsup": "^7.1.0",
      "typescript": "^5.1.6",
      "vitest": "^0.33.0"
   }
}
